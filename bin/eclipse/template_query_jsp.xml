<?xml version="1.0" encoding="UTF-8" standalone="no"?><templates><template autoinsert="true" context="jsp_new" deleted="false" description="for framework" enabled="true" name="New Query JSP">&lt;%@ page language="java" contentType="text/html; charset=UTF-8"&#13;
	pageEncoding="UTF-8"%&gt;&#13;
&lt;%@ include file="/common/taglibs.jsp" %&gt;&#13;
&lt;!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"&gt;&#13;
&lt;html xmlns="http://www.w3.org/1999/xhtml"&gt;&#13;
&lt;head&gt;&#13;
	&lt;title&gt;&lt;/title&gt;&#13;
	&lt;%@ include file="/common/header.jsp" %&gt;&#13;
&lt;/head&gt;&#13;
&lt;script type="text/javascript"&gt;&#13;
&lt;!--&#13;
$$(function(){&#13;
	_list_panel_obj=$$('#_list_panel').panel({&#13;
			height:430&#13;
		});&#13;
	_list_grid_obj=$$('#_list_table').datagrid({&#13;
		title:'#{tabel_title}',&#13;
		url:'$${ctx}/sm/server/server!search.action',&#13;
		singleSelect:true,&#13;
		height:350,&#13;
		idField:'id',&#13;
		pagination:true,&#13;
		pageSize:15,&#13;
		pageNumber:1,&#13;
		pageList:[10,15],&#13;
		rownumbers:true,&#13;
		sortName:'id',&#13;
	    sortOrder:'asc',&#13;
	    striped:true,&#13;
	    toolbar:[{&#13;
	        text:'&lt;s:text name="button.common.add"&gt;&lt;/s:text&gt;',&#13;
	        iconCls:'icon-add',&#13;
	        handler:function(){&#13;
	    		window.location='$${ctx}/#{action_mapping}/#{action_name}!forUpdate.action';&#13;
	        }&#13;
	    }]&#13;
	});&#13;
});&#13;
function searchByFilter(){&#13;
	var name=document.getElementById("filter_LIKES_name").value;&#13;
	var ip=document.getElementById("filter_LIKES_ip").value;&#13;
	_list_grid_obj.datagrid('reload',{&#13;
		filter_LIKES_ip:ip,&#13;
		filter_LIKES_name:name&#13;
		});&#13;
}&#13;
&#13;
function  editRow(id){&#13;
	window.location='$${ctx}/#{action_mapping}/#{action_name}!forUpdate.action?id='+id;&#13;
}&#13;
function  deleteRow(id){&#13;
	$$.messager.confirm("Confirm","&lt;s:text name='message.common.delete.confirm'&gt;&lt;/s:text&gt;",function(flag){&#13;
		if(flag){&#13;
			$$.ajax({&#13;
					url:'$${ctx}/#{action_mapping}/#{action_name}!delete.action?id='+id,&#13;
					type: 'GET',&#13;
					dataType:'json',&#13;
					error:function(){&#13;
						$$.messager.alert("&lt;s:text name='label.common.error'&gt;&lt;/s:text&gt;","&lt;s:text name='message.common.delete.failure'&gt;&lt;/s:text&gt;",'error');&#13;
					},&#13;
					success:function(data){&#13;
						if(data.success){&#13;
							_list_grid_obj.datagrid('reload',{});&#13;
							$$.messager.alert("&lt;s:text name='label.common.success'&gt;&lt;/s:text&gt;",data.message,'info');&#13;
						}else{&#13;
							$$.messager.alert("&lt;s:text name='label.common.error'&gt;&lt;/s:text&gt;",data.message,'error');&#13;
						}&#13;
					}&#13;
				}&#13;
			);&#13;
		}&#13;
	});&#13;
}&#13;
function actionFormatter(value,rowData,rowIndex){&#13;
	var id = rowData.serverId;&#13;
	var e = '&lt;img onclick="editRow('+id+')" src="$${ctx}/image/edit.gif" title="'+"&lt;s:text name='button.common.modify'&gt;&lt;/s:text&gt;"+'" style="cursor:hand;"/&gt;';   &#13;
    var d = '&lt;img onclick="deleteRow('+id+')" src="$${ctx}/image/delete.gif" title="'+"&lt;s:text name='button.common.delete'&gt;&lt;/s:text&gt;"+'" style="cursor:hand;"/&gt;';&#13;
    return e+'&amp;nbsp;'+d;&#13;
}&#13;
function viewResources(id){&#13;
	window.location="$${ctx}/#{action_mapping}/#{action_name}!forView.action?id="+id;&#13;
}&#13;
&#13;
//--&gt;&#13;
&lt;/script&gt;&#13;
&lt;body&gt;&#13;
&lt;div id="_list_panel" style="padding:10px;"&gt;&#13;
&lt;div id="filter"&gt;&#13;
	&lt;table border="0"&gt;&#13;
		&lt;tr&gt;&#13;
			&lt;td&gt;&lt;s:text name='#{condition_lable_1}'&gt;&lt;/s:text&gt;:&lt;/td&gt;&#13;
			&lt;td&gt;&lt;input type="text" name="filter_LIKES_#{condition_1}" value="" size="9"/&gt;&lt;/td&gt;&#13;
			&lt;td&gt;&lt;s:text name='#{condition_lable_2}'&gt;&lt;/s:text&gt;:&lt;/td&gt;&#13;
			&lt;td&gt;&lt;input type="text" name="filter_LIKES_#{condition_2}" value="" size="9"/&gt;&lt;/td&gt;&#13;
			&lt;td&gt;&lt;a href="#" class="easyui-linkbutton" iconCls="icon-search" onclick="searchByFilter();"&gt;&lt;s:text name='button.common.search'&gt;&lt;/s:text&gt;&lt;/a&gt;&lt;/td&gt;&#13;
		&lt;/tr&gt;&#13;
	&lt;/table&gt;&#13;
&lt;/div&gt;&#13;
&lt;div id="content"&gt;&#13;
&lt;table id="_list_table"&gt;&#13;
	&lt;thead&gt;&#13;
		&lt;tr&gt;&#13;
			&lt;th field="#{col_1}" width="200" align="middle" sortable="true"&gt;#{col_lable_1}&lt;/th&gt;&#13;
			&lt;th field="#{col_2}" width="50" align="middle" sortable="true"&gt;#{col_lable_2}&lt;/th&gt;&#13;
			&lt;th field="#{col_3}" width="100" align="middle"&gt;#{col_lable_3}&lt;/th&gt;&#13;
			&lt;th field="#{col_4}" width="50" align="middle"&gt;#{col_lable_4}&lt;/th&gt;&#13;
			&lt;th field="#{col_5}" width="50" align="middle"&gt;#{col_lable_5}&lt;/th&gt;&#13;
			&lt;th field="#{col_6}" width="150" align="middle"&gt;#{col_lable_6}&lt;/th&gt;&#13;
			&lt;th field="#{col_7}" width="100" align="middle" formatter="actionFormatter"&gt;&lt;s:text name='label.sm.common.action'&gt;&lt;/s:text&gt;&lt;/th&gt;&#13;
		&lt;/tr&gt;&#13;
	&lt;/thead&gt;&#13;
&lt;/table&gt;&#13;
&lt;/div&gt;&#13;
&lt;/div&gt; &#13;
&lt;/body&gt;&#13;
&lt;/html&gt;&#13;
</template></templates>